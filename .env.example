
# Database
DATABASE_URL=your_database_url_here

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# Razorpay (if using payments)
RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Security
SESSION_SECRET=generate_a_random_32_character_string_here
API_KEY_SECRET=generate_another_random_32_character_string

# Environment
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
API_RATE_LIMIT_MAX=50
ANALYSIS_RATE_LIMIT_MAX=10

# CORS Settings (production only)
ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com
